{
	"$schema": "./node_modules/turbo/schema.json",
	"tasks": {
		"dev": {
			"cache": false,
			"persistent": true
		},
		"build": {
			"dependsOn": ["^build"],
			"inputs": [".env*"],
			"outputs": [".next/**", "!.next/cache/**", "build/**"]
		},
		"test": {
			"dependsOn": ["^test"],
			"outputs": ["coverage/**"]
		},
		"lint": {},
		"lint:fix": {}
	}
}
